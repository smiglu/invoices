{
  "name": "vue2",
  "version": "1.0.0",
  "private": true,
  "description": "A Vue.js project together with RxDB",
  "author": "pubkey",
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:serve": "ws -p 8888 -d dist/",
    "dev": "vue-cli-service serve",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
    "postbuild": "cp test/multitab.html ./dist/multitab.html",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "server": "pouchdb-server --host 0.0.0.0 -p 10101 -m -d /tmp/pouchdb-server/ -n true",
    "start": "concurrently \"npm run server\" \"npm run dev\"",
    "test": "concurrently \"npm run build:serve \" \"npm run test:run\" --kill-others --success first",
    "test:run": "testcafe chromium test/ --selector-timeout 30000"
  },
  "main": "background.js",
  "dependencies": {
    "pouchdb-adapter-http": "7.2.1",
    "pouchdb-adapter-idb": "7.2.1",
    "pouchdb-server": "4.2.0",
    "random-int": "2.0.1",
    "register-service-worker": "1.6.2",
    "rxdb": "^8.8.0",
    "vue": "2.6.11",
    "vue-class-component": "7.2.3",
    "vue-i18n": "^8.0.0",
    "vue-property-decorator": "8.4.0",
    "vue-router": "3.1.6",
    "vuetify": "^2.2.18"
  },
  "devDependencies": {
    "@types/node": "13.7.7",
    "@types/pouchdb-adapter-http": "6.1.3",
    "@types/pouchdb-adapter-idb": "6.1.3",
    "@types/webpack": "^4.4.0",
    "@vue/cli-plugin-pwa": "4.2.3",
    "@vue/cli-plugin-typescript": "4.2.3",
    "@vue/cli-service": "4.2.3",
    "concurrently": "5.1.0",
    "electron": "^6.0.0",
    "less": "3.11.1",
    "less-loader": "5.0.0",
    "local-web-server": "4.0.0",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "testcafe": "1.8.2",
    "typescript": "3.8.3",
    "vue-cli-plugin-electron-builder": "~1.4.6",
    "vue-cli-plugin-i18n": "~0.6.1",
    "vue-cli-plugin-vuetify": "~2.0.5",
    "vue-template-compiler": "2.6.11",
    "vuetify-loader": "^1.3.0"
  }
}
